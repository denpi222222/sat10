# Alchemy API Key (get from https://www.alchemy.com/)
# Single key is used across the app
NEXT_PUBLIC_ALCHEMY_API_KEY_1=your_alchemy_key_here

# WalletConnect Project ID (get from https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Security Configuration
NEXT_PUBLIC_SECURITY_ENABLED=true
NEXT_PUBLIC_RATE_LIMIT_ENABLED=true
NEXT_PUBLIC_CSRF_ENABLED=true

# Debug Mode (set to true for development)
NEXT_PUBLIC_DEBUG_SECURITY=false 

# Enable Trusted Types policies (optional; may affect some mobile browsers)
NEXT_PUBLIC_TRUSTED_TYPES_ENABLED=false